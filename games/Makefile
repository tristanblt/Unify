all:
	make -C solarfox/
	make -C nibbler/
	make -C unify_showcase/

re:
	make re -C solarfox/
	make re -C nibbler/
	make re -C unify_showcase/

clean:
	make clean -C solarfox/
	make clean -C nibbler/
	make clean -C unify_showcase/

fclean:
	make fclean -C solarfox/
	make fclean -C nibbler/
	make fclean -C unify_showcase/